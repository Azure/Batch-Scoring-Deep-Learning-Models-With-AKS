{
    "apiVersion": "apps/v1beta1",
    "kind": "Deployment",
    "metadata": {
        "labels": {
            "purpose": "dequeue_messages"
        },
        "name": "dequeuer"
    },
    "spec": {
        "replicas": 3,
        "template": {
            "metadata": {
                "labels": {
                    "app": "dequeuer"
                }
            },
            "spec": {
                "containers": [
                    {
                        "env": [
                            {
                                "name": "LB_LIBRARY_PATH",
                                "value": "$LD_LIBRARY_PATH:/usr/local/nvidia/lib64:/opt/conda/envs/py3.6/lib"
                            },
                            {
                                "name": "DP_DISABLE_HEALTHCHECKS",
                                "value": "xids"
                            },
                            {
                                "name": "STYLE_WEIGHT",
                                "value": "100000000"
                            },
                            {
                                "name": "CONTENT_WEIGHT",
                                "value": "1"
                            },
                            {
                                "name": "NUM_STEPS",
                                "value": "80"
                            },
                            {
                                "name": "IMAGE_SIZE",
                                "value": "360"
                            },
                            {
                                "name": "VIDEO_NAME",
                                "value": "orangutan"
                            },
                            {
                                "name": "SUBSCRIPTION_ID",
                                "value": "edf507a2-6235-46c5-b560-fd463ba2e771"
                            },
                            {
                                "name": "RESOURCE_GROUP",
                                "value": "jiatahappypath01"
                            },
                            {
                                "name": "REGION",
                                "value": "eastus"
                            },
                            {
                                "name": "STORAGE_ACCOUNT_NAME",
                                "value": "jiatahappypath01"
                            },
                            {
                                "name": "STORAGE_ACCOUNT_KEY",
                                "value": "SyhdAXUS/KuY9l+xjtG7OVcUzZQMpFAlunKhDnx5YAOhW6J+lubxN6YS9j5QXhtkBAR0cHKjtmB/66YnUhTJrQ=="
                            },
                            {
                                "name": "STORAGE_CONTAINER_NAME",
                                "value": "aks"
                            },
                            {
                                "name": "SB_SHARED_ACCESS_KEY_NAME",
                                "value": "RootManageSharedAccessKey"
                            },
                            {
                                "name": "SB_SHARED_ACCESS_KEY_VALUE",
                                "value": "qwwUGO9i71i/aMSI2Pa+f2V5Bi1VH9yaGYjmoSiB800="
                            },
                            {
                                "name": "SB_NAMESPACE",
                                "value": "happypath-namespace01"
                            },
                            {
                                "name": "SB_QUEUE",
                                "value": "happypath-queue01"
                            }
                        ],
                        "image": "jiata/batchscoringdl_receiver:latest",
                        "name": "dequeuer",
                        "ports": [
                            {
                                "containerPort": 433
                            }
                        ],
                        "resources": {
                            "limits": {
                                "alpha.kubernetes.io/nvidia-gpu": 1
                            },
                            "requests": {
                                "alpha.kubernetes.io/nvidia-gpu": 1
                            }
                        },
                        "volumeMounts": [
                            {
                                "mountPath": "/usr/local/nvidia",
                                "name": "nvidia"
                            }
                        ]
                    }
                ],
                "volumes": [
                    {
                        "hostPath": {
                            "path": "/usr/local/nvidia"
                        },
                        "name": "nvidia"
                    }
                ]
            }
        }
    }
}

